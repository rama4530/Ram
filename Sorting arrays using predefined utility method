package module1;

import java.util.Arrays;

public class arryas {

	public static void main(String a[]){
		int a1[]={80,20,90,70,30,10};
		System.out.println("-----Array List--------");
		for(int x:a1){
			System.out.print(x+",");
		}
		System.out.println();
		Arrays.sort(a1);
		System.out.println("-----Array List After Sorting--------");
		for(int y:a1){
			
			System.out.print(y+",");
		}
	}
}
